{
  "name": "namingenie",
  "displayName": "NaminGenie",
  "description": "suggest var",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "namingenie.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "namingenie.enterCredentials",
        "title": "Enter Translation Service Credentials"
      }
    ],
    "configuration": {
      "properties": {
          "namingenie.clientId": {
              "type": "string",
              "default": "",
              "description": "Client ID for the translation service"
          },
          "namingenie.clientSecret": {
              "type": "string",
              "default": "",
              "description": "Client Secret for the translation service"
          }
      }
  }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/jest": "^29.5.11",
    "axios": "^1.6.2",
    "chai": "^4.3.10",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1"
  }
}
